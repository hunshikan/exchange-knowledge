/**app.wxss**/
.container {
    width: 100%;
    min-height: 100vh;
    overflow: hidden;
    box-sizing: border-box;
    background: #f4f5f6;
}

section,
article,
header,
footer,
div {
    display: block;
    text-align: left;
}

/* ==== 公用样式 ==== */
/* 单行截断 */
.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

/* 多行截断 */
.line-clamp {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow: hidden;
    -webkit-line-clamp: 3;
}


.submit-btn {
    display: block;
    height: 48px;
    border: 0;
    border-radius: 5px;
    text-align: center;
    color: #fff;
    line-height: 48px;
    background: #51a938;
}

.submit-btn.disabled {
    color: #fff;
    background: #e0e0e0;
}

/* ==== 图标 ==== */
@font-face {
    font-family: "iconfont";
    src: url('data:application/x-font-woff;charset=utf-8;base64,') format('woff');
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

